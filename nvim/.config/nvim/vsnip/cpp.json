{
  "inclde": {
    "prefix": ["in"],
    "body": [
      "#include <$1>"
    ],
    "description": "include"
  },
  "using namespace": {
    "prefix": ["un"],
    "body": [
      "using namespace ${1:std};"
    ],
    "description": "using namespace"
  },
  "if": {
    "prefix": ["if"],
    "body": [
      "if ($1) {",
      "\t$2",
      "}"
    ],
    "description": "if"
  },
  "if else": {
    "prefix": ["ife"],
    "body": [
      "if ($1) {",
      "\t$2",
      "} else {",
      "\t$3",
      "}"
    ],
    "description": "if else"
  },
  "int main()": {
    "prefix": ["intm"],
    "body": [
      "int main() {",
      "\t$1",
      "\treturn 0;",
      "}"
    ],
    "description": "int main()"
  },
  "cout": {
    "prefix": ["co"],
    "body": [
      "cout << $1 << endl;"
    ],
    "description": "cout"
  },
  "for loop": {
    "prefix": ["fo"],
    "body": [
      "for (${1:int i = 0}; ${2:i < n}; ${3:i++}) {",
      "\t$4",
      "}"
    ],
    "description": "for loop"
  },
  "vec": {
    "prefix": ["vec"],
    "body": [
      "vector<$1>"
    ],
    "description": "vec"
  }
}
