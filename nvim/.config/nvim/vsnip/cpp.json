{
  "inclde": {
    "prefix": ["in"],
    "body": [
      "#include <$1>"
    ],
    "description": "include"
  },
  "using namespace": {
    "prefix": ["un"],
    "body": [
      "using namespace ${1:std};"
    ],
    "description": "using namespace"
  },
  "if": {
    "prefix": ["if"],
    "body": [
      "if ($1) {",
      "    $2",
      "}"
    ],
    "description": "if"
  },
  "if else": {
    "prefix": ["ife"],
    "body": [
      "if ($1) {",
      "    $2",
      "} else {",
      "    $3",
      "}"
    ],
    "description": "if else"
  },
  "int main()": {
    "prefix": ["mai"],
    "body": [
      "int main() {",
      "    $1",
      "\treturn 0;",
      "}"
    ],
    "description": "int main()"
  },
  "cout": {
    "prefix": ["co"],
    "body": [
      "cout << ${1:$TM_SELECTED_TEXT} << endl;"
    ],
    "description": "cout"
  },
  "for loop": {
    "prefix": ["fo"],
    "body": [
      "for (${1:int i = 0}; ${2:i < n}; ${3:i++}) {",
      "    $4",
      "}"
    ],
    "description": "for loop"
  },
  "vec": {
    "prefix": ["vec"],
    "body": [
      "vector<$1>"
    ],
    "description": "vec"
  },
  "TreeNode": {
    "prefix": ["stn"],
    "body": [
      "struct TreeNode {",
      "    int val;",
      "    TreeNode* left;",
      "    TreeNode* right;",
      "    TreeNode(int v) : val(v) {}",
      "};"
    ],
    "description": "TreeNode"
  }
}
